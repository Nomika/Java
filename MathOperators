package org.example;

import org.w3c.dom.ls.LSOutput;

public class Operatory_Math {
    public static void main(String[] args) {

        int pierwsza = 4;
        int druga = 6;
        pierwsza+=pierwsza+druga;
        float trzecia = 4.5F;

        int dodawanie = pierwsza + druga;
        int odejmowanie = pierwsza - druga;
        int mnozenie = pierwsza * druga;
        float dzielenie = pierwsza / druga;
        float dzielenie1 = pierwsza/trzecia;
        int modulo = druga%pierwsza;



        System.out.println(dodawanie);
        System.out.println(odejmowanie);
        System.out.println(mnozenie);
        System.out.println(dzielenie);
        System.out.println(dzielenie1);
        System.out.println(modulo);
        System.out.println(pierwsza);

        System.out.println(dodawanie);
        System.out.println(odejmowanie);

    }
}
