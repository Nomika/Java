package org.example;

public class Konwersja {
    public static void main(String[] args) {

        int a = 2;
        double b = 4.23;

        double c  = a/b;

        int d = (int) (a/b);
        int e = a/(int) b;

        System.out.println(c);
        System.out.println(d);
        System.out.println(e);
    }
}
